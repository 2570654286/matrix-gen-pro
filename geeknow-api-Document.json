                video_url = data.get("url") or data.get("output") or data.get("video_url")
                
                # æœ‰æ—¶å€™é“¾æ¥è—åœ¨ data å­—æ®µé‡Œ
                if not video_url and "data" in data and isinstance(data["data"], dict):
